    <!-- JavaScript ÉPICO -->
    <script>
        // Variables globales
        let particles = [];
        let cursorTrails = [];
        let currentTestimonial = 0;
        
        // Cursor personalizado épico
        document.addEventListener('mousemove', (e) => {
            const cursor = document.querySelector('.custom-cursor');
            cursor.style.left = e.clientX + 'px';
            cursor.style.top = e.clientY + 'px';
            
            // Crear trail del cursor
            createCursorTrail(e.clientX, e.clientY);
        });

        function createCursorTrail(x, y) {
            const trail = document.createElement('div');
            trail.className = 'cursor-trail';
            trail.style.left = x + 'px';
            trail.style.top = y + 'px';
            document.body.appendChild(trail);
            
            setTimeout(() => {
                trail.remove();
            }, 500);
        }

        // Partículas épicas en el hero
        function createParticles() {
            const container = document.getElementById('particles');
            
            setInterval(() => {
                const particle = document.createElement('div');
                particle.className = 'particle';
                particle.style.left = Math.random() * 100 + '%';
                particle.style.animationDuration = (Math.random() * 3 + 2) + 's';
                container.appendChild(particle);
                
                setTimeout(() => {
                    particle.remove();
                }, 5000);
            }, 300);
        }

        // Contador de velocidad épico
        function animateSpeedCounter() {
            const counter = document.getElementById('speed-counter');
            let current = 0;
            const target = 200;
            const increment = target / 100;
            
            const timer = setInterval(() => {
                current += increment;
                if (current >= target) {
                    current = target;
                    clearInterval(timer);
                }
                counter.textContent = Math.floor(current);
            }, 30);
        }

        // Seleccionar plan épico
        function selectPlan(name, speed, price) {
            // Efectos visuales
            document.body.style.background = 'linear-gradient(45deg, #FF6B35, #00F5FF)';
            
            setTimeout(() => {
                document.body.style.background = '';
            }, 500);
            
            // Mensaje épico
            alert(`🚀 SELECCIONASTE EL PLAN ${name.toUpperCase()}! ${speed} por ${price}. 
                   ¡Preparate para la velocidad de la luz! ⚡`);
            
            // Abrir WhatsApp con mensaje personalizado
            const message = encodeURIComponent(
                `🚀 QUIERO EL PLAN ${name.toUpperCase()}! 
                ${speed} por ${price}/mes. 
                Estoy listo para tener superpoderes digitales! ⚡🎮`
            );
            
            window.open(`https://wa.me/5492223528209?text=${message}`, '_blank');
        }

        // Speed test épico
        function runSpeedTest() {
            const needle = document.getElementById('speed-needle');
            const result = document.getElementById('current-speed');
            const message = document.getElementById('speed-message');
            const button = document.querySelector('.test-button');
            
            button.textContent = '⚡ ANALIZANDO...';
            button.disabled = true;
            
            // Simular test realista
            let currentSpeed = 0;
            const finalSpeed = Math.floor(Math.random() * 50) + 10; // 10-60 MB
            
            const testInterval = setInterval(() => {
                currentSpeed += Math.random() * 5;
                if (currentSpeed >= finalSpeed) {
                    currentSpeed = finalSpeed;
                    clearInterval(testInterval);
                    
                    // Resultado épico
                    result.textContent = finalSpeed + ' MB';
                    
                    if (finalSpeed < 25) {
                        message.innerHTML = '🐌 TU INTERNET ES UNA TORTUGA! Con nosotros tendrías 200MB! 🚀';
                        message.style.color = '#FF1744';
                    } else if (finalSpeed < 50) {
                        message.innerHTML = '😐 No está mal, pero con Brandsen Digital serías IMPARABLE! ⚡';
                        message.style.color = '#FFD700';
                    } else {
                        message.innerHTML = '🤔 Está bien, pero ¿ya probaste nuestros 200MB? ¡ES OTRO NIVEL! 👑';
                        message.style.color = '#00FF94';
                    }
                    
                    button.textContent = '🚀 QUIERO MEJORAR MI INTERNET';
                    button.disabled = false;
                    button.onclick = () => {
                        window.open('https://wa.me/5492223528209?text=🐌 Mi internet es lento! Quiero los 200MB de Brandsen Digital!', '_blank');
                    };
                }
                
                // Mover aguja
                const angle = (currentSpeed / 100) * 180 - 90;
                needle.style.transform = `translateX(-50%) rotate(${angle}deg)`;
                result.textContent = Math.floor(currentSpeed) + ' MB';
            }, 100);
        }

        // Modo gaming épico
        function gameMode() {
            document.body.style.filter = 'hue-rotate(120deg)';
            document.body.style.animation = 'rainbow-text 1s ease-in-out';
            
            alert('🎮 MODO BESTIA ACTIVADO! 👑 Ahora sos IMPARABLE en todos los juegos! ⚡');
            
            setTimeout(() => {
                document.body.style.filter = '';
                document.body.style.animation = '';
            }, 2000);
            
            // Abrir WhatsApp modo gamer
            const message = encodeURIComponent(
                '🎮 QUIERO ACTIVAR MODO BESTIA! Necesito el internet más rápido para gaming. ¡Háblame de los planes! ⚡👑'
            );
            window.open(`https://wa.me/5492223528209?text=${message}`, '_blank');
        }

        // Acciones de contacto épicas
        function contactAction(type) {
            const actions = {
                phone: () => {
                    window.open('tel:2223528209');
                },
                whatsapp: () => {
                    const message = encodeURIComponent('🚀 Hola! Quiero superpoderes digitales con Brandsen Digital! Contame todo sobre los planes épicos! ⚡');
                    window.open(`https://wa.me/5492223528209?text=${message}`, '_blank');
                },
                location: () => {
                    window.open('https://maps.app.goo.gl/3R2PPrDTP2AwdgPM7', '_blank');
                },
                email: () => {
                    window.open('mailto:info@brandsendigital.com.ar?subject=🚀 Quiero Internet Épico!&body=Hola! Estoy listo para tener superpoderes digitales!');
                }
            };
            
            actions[type]();
        }

        // Efectos de sonido simulados
        function playSound() {
            // Agregar clase de efecto sonoro
            event.target.classList.add('sound-effect');
            setTimeout(() => {
                event.target.classList.remove('sound-effect');
            }, 500);
        }

        // Header scroll épico
        window.addEventListener('scroll', () => {
            const header = document.getElementById('header');
            if (window.scrollY > 100) {
                header.classList.add('scrolled');
            } else {
                header.classList.remove('scrolled');
            }
        });

        // Navegación suave épica
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Intersección observer para animaciones
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                    
                    // Animar contadores de velocidad en las cards
                    const speedElement = entry.target.querySelector('[data-speed]');
                    if (speedElement) {
                        const targetSpeed = parseInt(speedElement.dataset.speed);
                        animateNumber(speedElement, targetSpeed);
                    }
                }
            });
        }, { threshold: 0.3 });

        // Observar elementos para animación
        document.querySelectorAll('.plan-card, .contact-card, .testimonial').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(50px)';
            el.style.transition = 'all 0.6s ease';
            observer.observe(el);
        });

        // Animar números
        function animateNumber(element, target) {
            let current = 0;
            const increment = target / 50;
            const timer = setInterval(() => {
                current += increment;
                if (current >= target) {
                    current = target;
                    clearInterval(timer);
                }
                element.textContent = Math.floor(current) + 'MB';
            }, 30);
        }

        // Slider de testimonios automático
        function autoSlideTestimonials() {
            const testimonials = document.querySelectorAll('.testimonial');
            setInterval(() => {
                testimonials[currentTestimonial].style.opacity = '0.5';
                currentTestimonial = (currentTestimonial + 1) % testimonials.length;
                testimonials[currentTestimonial].style.opacity = '1';
                testimonials[currentTestimonial].scrollIntoView({ 
                    behavior: 'smooth', 
                    block: 'center',
                    inline: 'center'
                });
            }, 5000);
        }

        // Ping simulado para gaming
        function updatePing() {
            const pingDisplay = document.getElementById('ping-display');
            setInterval(() => {
                const ping = Math.floor(Math.random() * 10) + 5; // 5-15ms
                pingDisplay.textContent = `PING: ${ping}ms ⚡`;
                
                if (ping < 10) {
                    pingDisplay.style.color = 'var(--neon-green)';
                } else {
                    pingDisplay.style.color = 'var(--warning)';
                }
            }, 2000);
        }

        // Efectos de hover épicos en plan cards
        document.querySelectorAll('.plan-card').forEach(card => {
            card.addEventListener('mouseenter', () => {
                card.style.transform = 'translateY(-20px) scale(1.02) rotate(2deg)';
                card.style.boxShadow = '0 30px 60px rgba(0, 245, 255, 0.6)';
            });
            
            card.addEventListener('mouseleave', () => {
                card.style.transform = '';
                card.style.boxShadow = '';
            });
        });

        // Efecto matrix en background
        function createMatrixEffect() {
            const chars = '01';
            setInterval(() => {
                const span = document.createElement('span');
                span.textContent = chars[Math.floor(Math.random() * chars.length)];
                span.style.position = 'fixed';
                span.style.left = Math.random() * 100 + 'vw';
                span.style.top = '-20px';
                span.style.color = 'var(--neon-blue)';
                span.style.fontSize = '20px';
                span.style.opacity = '0.1';
                span.style.pointerEvents = 'none';
                span.style.zIndex = '-1';
                span.style.animation = 'particle-float 4s linear forwards';
                
                document.body.appendChild(span);
                
                setTimeout(() => {
                    span.remove();
                }, 4000);
            }, 100);
        }

        // Inicializar todo cuando carga la página
        window.addEventListener('load', () => {
            createParticles();
            animateSpeedCounter();
            autoSlideTestimonials();
            updatePing();
            createMatrixEffect();
            
            // Mensaje de bienvenida épico
            setTimeout(() => {
                if (Math.random() > 0.7) { // 30% de probabilidad
                    alert('🚀 BIENVENIDO A LA EXPERIENCIA MÁS ÉPICA DE INTERNET! ¿Listo para cambiar tu vida digital? ⚡');
                }
            }, 3000);
        });

        // Easter eggs
        let clickCount = 0;
        document.addEventListener('click', () => {
            clickCount++;
            if (clickCount === 10) {
                document.body.style.animation = 'rainbow-text 2s ease-in-out';
                alert('🎉 EASTER EGG DESBLOQUEADO! ¡Sos un usuario épico! 🎮');
                setTimeout(() => {
                    document.body.style.animation = '';
                }, 2000);
            }
        });

        // Konami code
        let konamiCode = [];
        const konamiSequence = ['ArrowUp', 'ArrowUp', 'ArrowDown', 'ArrowDown', 'ArrowLeft', 'ArrowRight', 'ArrowLeft', 'ArrowRight', 'KeyB', 'KeyA'];
        
        document.addEventListener('keydown', (e) => {
            konamiCode.push(e.code);
            if (konamiCode.length > konamiSequence.length) {
                konamiCode.shift();
            }
            
            if (JSON.stringify(konamiCode) === JSON.stringify(konamiSequence)) {
                document.body.style.background = 'linear-gradient(45deg, #FF073A, #00F5FF, #39FF14)';
                document.body.style.animation = 'rainbow-text 5s ease-in-out infinite';
                alert('🎮 KONAMI CODE ACTIVATED! ¡MODO DESARROLLADOR DESBLOQUEADO! 👑');
            }
        });
    </script>
</body>
</html>
